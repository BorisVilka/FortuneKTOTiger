<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.ktotiger.acrashjet.StartFragment"
        android:label="StartFragment" >
        <action
            android:id="@+id/action_startFragment_to_gameFragment"
            app:destination="@id/gameFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_startFragment_to_webActivity"
            app:destination="@id/webActivity" />
    </fragment>
    <fragment
        android:id="@+id/gameFragment"
        android:name="com.ktotiger.acrashjet.GameFragment"
        android:label="fragment_game"
        tools:layout="@layout/fragment_game" />
    <activity
        android:id="@+id/webActivity"
        android:name="com.ktotiger.acrashjet.WebActivity"
        android:label="WebActivity" />
</navigation>